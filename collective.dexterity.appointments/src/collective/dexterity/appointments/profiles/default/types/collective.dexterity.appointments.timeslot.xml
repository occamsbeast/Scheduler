<?xml version="1.0"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n" name="collective.dexterity.appointments.timeslot" meta_type="Dexterity FTI" i18n:domain="collective.dexterity.appointments">
  <!-- Basic metadata -->
  <property name="title" i18n:translate="">Time Slot</property>
  <property name="description" i18n:translate="">A Time Slot for appointments</property>
  <property name="content_icon">misc_/CMFFormController/tool.gif</property>
  <!-- navigation_icon.png</property>-->
  <property name="global_allow">False</property>
  <property name="filter_content_types">True</property>
  <property name="allowed_content_types">
    <element value="collective.dexterity.appointments.person"/>
  </property>
  <property name="allow_discussion">False</property>
  <!-- schema interface -->
  <property name="schema">collective.dexterity.appointments.interfaces.ITimeSlot</property>
  <!-- class used for content items -->
  <property name="klass">collective.dexterity.appointments.content.timeslot.TimeSlot</property>
  <property name="add_permission">cmf.AddPortalContent</property>
  <property name="behaviors">
    <element value="plone.app.content.interfaces.INameFromTitle"/>
    <element value="collective.dexterity.appointments.interfaces.IClone"/>
  </property>
  <!-- View information -->
  <property name="default_view">view</property>
  <property name="default_view_fallback">False</property>
  <property name="view_methods">

  </property>
  <!-- Method aliases -->
  <alias from="(Default)" to="(selected layout)"/>
  <alias from="edit" to="@@edit"/>
  <alias from="sharing" to="@@sharing"/>
  <alias from="view" to="@@view"/>
  <!-- Actions -->
  <action title="View" action_id="view" category="object" condition_expr="" url_expr="string:${object_url}/" visible="True">
    <permission value="View"/>
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr="" url_expr="string:${object_url}/edit" visible="True">
    <permission value="View"/>
  </action>
</object>
