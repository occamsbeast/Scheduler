       name="solgemafullcalendar/colorpicker/css"/>
   <cmf:registerDirectory
       name="solgemafullcalendar/colorpicker/images"/>
   <cmf:registerDirectory
       name="solgemafullcalendar/colorpicker/js"/>

</configure>
